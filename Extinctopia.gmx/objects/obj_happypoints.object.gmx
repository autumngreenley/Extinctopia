winCondition=false;             //Sets the win condition to false
loseCondition=false;            //Sets the lose condition to false

if(kid_disposition==0){                 //If the kid is happy...
    happypoints=irandom_range(65,75);   //They have a HP of between 65-75
    obj_kid.sprite_index=spr_happy;     //And it sets their sprite to a happy one
}
if(kid_disposition==1){                 //If the kid is mad...
    happypoints=irandom_range(45,55);   //They have a HP between 45-55
    obj_kid.sprite_index=spr_mad;       //And it sets their sprite to an angry one
}
