        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Time Control

if(global.stopTime==0){     //If time isn't stopped
    if(global.mins&gt;0){      //And if there is still more minutes in the hour
        global.mins-=1      //Then a minute ticks down
    }
}

if(global.time&lt;13){   //If the time is less than 1pm
    if(global. mins==0){        //And if there are no more minutes in the hour
        global.time+=1;         //The hour goes up
        global.mins=mins_max;        //The minutes are reset to full value
        global.move+=1;         //The two-hour counter increases
    }
}


if(global.time&gt;=13 &amp;&amp; global.time&lt;=17){    //If the time is more than noon and less then 5pm
    if(global.mins=0){                  //And if there are no more minutes in the hour
        global.time2+=1                 //The display-time increases (1pm, 2pm, etc instead of 13pm, 14pm)
        global.time+=1                  //The overall time increases (13, 14...)
        global.mins=mins_max;           //The minutes are reset to full value
        global.move+=1                  //The two-hour counter increases
    }
}

if(global.time==17){                  //When it turns to 5pm...
//if(global.time==9){ //*for testing*
    room_goto(rm_boss_room);        //go to the bosses room
    global.stopTime=1;              //stop time
    audio_stop_sound(snd_overworld_music);
    instance_destroy();             //destroy the master object
}

if(global.time==13 &amp;&amp; global.move==1){
    global.stopTime=1;
    time_message=true;
    global.stopMovement=1;
    obj_player.stopIllu=1;
    instance_create(0,568,obj_messagebox);
    global.move=0;          //resets the two-hour counter
}

if(global.move==2){                 //If it's been two hours
//    show_message('2 hours');    //shows the user a message
    time_message=true;
    global.stopTime=1;
    global.stopMovement=1;
    obj_player.stopIllu=1;
    instance_create(0,568,obj_messagebox);
    global.move=0;          //resets the two-hour counter
}
</string>
